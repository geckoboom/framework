{
    "name": "whirlwind-framework/framework",
    "description": "Whirlwind DDD micro framework",
    "keywords": [
        "framework",
        "ddd",
        "micro framework"
    ],
    "authors": [
        {
            "name": "Alexander Stepanenko",
            "email": "alex.stepanenko@gmail.com"
        }
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": "~7.4||>=8.0",
        "ext-json": "*",
        "psr/container": "^1.1 || ^2.0",
        "ext-mbstring": "*",
        "psr/http-server-middleware": "1.0.1",
        "psr/http-factory": "1.0.1",
        "psr/http-client": "1.0.1"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.2",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Whirlwind\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    }
}
