{
    "name": "whirlwind-framework/framework",
    "description": "Whirlwind DDD micro framework",
    "keywords": [
        "framework",
        "ddd",
        "micro framework"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "require": {
        "php": "~7.4",
        "ext-mongodb": "*",
        "ext-json": "*",
        "league/route": "5.x-dev",
        "laminas/laminas-diactoros": "2.4.x-dev",
        "laminas/laminas-httphandlerrunner": "1.3.x-dev",
        "league/container": "3.3.1",
        "psr/cache": "^1.0"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.2",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Whirlwind\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    }
}
